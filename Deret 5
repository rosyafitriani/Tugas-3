public class Deret5 {
    public static void main(String[] args) {
        for (int i = 5; i >= 0; i--) {
            for (int a = 0; a < i; a++) {
                System.out.print("*");
            }

            for (int b = i; b <= 5; b++) {
                System.out.print(b);
            }
            System.out.println(); 
        }
    }
}
