public class Deret2 {
    public static void main(String[] args) {
        for (int i = 1; i <=1; i++) {
            for (int a = 1; a <= 6; a++){
                System.out.print(a + " ");
            }
        System.out.println();

        for (int a = 2; a <= 7; a++){
            System.out.print(a + " ");
        }
        System.out.println();

        for (int a = 3; a <= 8; a++){
        System.out.print(a + " ");
        }
        
        System.out.println();
        }
    }
}
