public class Deret6 {
    public static void main(String[] args) {
        int a = 1, b = 1, c = 2; 
        System.out.print(a + " " + b + " " + c + " ");

        for (int i = 4; i <= 9; i++) {
            int next = a + b + c; 
            System.out.print(next + " "); 
            a = b; 
            b = c; 
            c = next; 
        }
    }
}
